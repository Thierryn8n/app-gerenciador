{
  "framework": "nextjs",
  "installCommand": "npm ci --legacy-peer-deps || npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "vercel-node@20.0.0"
    }
  },
  "env": {
    "ENABLE_EXPERIMENTAL_COREPACK": "0",
    "NPM_CONFIG_PACKAGE_LOCK": "false"
  }
}